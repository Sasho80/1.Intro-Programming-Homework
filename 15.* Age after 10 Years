using System;
using System.Globalization;

class Age
{
    static void Main()
    {
        DateTime now = DateTime.Now;
        Console.WriteLine("Enter your year in format d.MM.yyyy:");
        string BirthDay = Console.ReadLine();
        DateTime BirthDayDT =DateTime.Parse(BirthDay);
        int AgeNow = Math.Abs(DateTime.Now.Year-BirthDayDT.Year);
        if (DateTime.Now.Month > BirthDayDT.Month)
        {
           AgeNow=AgeNow + 1;
        }
        else if (DateTime.Now.Month<BirthDayDT.Month)
        {
            if (DateTime.Now.Day < BirthDayDT.Day || DateTime.Now.Day > BirthDayDT.Day)
            {
               AgeNow = AgeNow - 1; 
            }
        }
        //else if (DateTime.Now.Month == BirthDayDT.Month)
        //{
        //    if (DateTime.Now.Day == BirthDayDT.Day)
        //    {
        //    }
        //}
        int AgeAfterTenYears = AgeNow + 10;
        Console.WriteLine("{0}" , AgeNow);
        Console.WriteLine("{0}" , AgeAfterTenYears);
    }
}
